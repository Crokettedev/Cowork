<form method="post" action="{{ path('admin_supply_food_delete', {'id': supply_food.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ supply_food.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
