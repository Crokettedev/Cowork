<form method="post" action="{{ path('admin_place_public_delete', {'id': place_public.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ place_public.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
